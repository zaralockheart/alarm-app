<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.alarmapp.ui.alarm.AlarmFragment"
        android:label="Penggera"
        tools:layout="@layout/fragment_home" >
    </fragment>

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.example.alarmapp.ui.weatherforecast.WeatherForecastFragment"
        android:label="Cuaca"
        tools:layout="@layout/fragment_weather_forecast" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.example.alarmapp.ui.note.NoteFragment"
        android:label="Nota"
        tools:layout="@layout/fragment_notifications" />
</navigation>